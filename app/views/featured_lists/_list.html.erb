<h2><%= @featured_list.name %> <%= @featured_list.month %> <%= @featured_list.year %></h2>
<ul class="list-unstyled">
<% @featured_list.songs.each.with_index do |t, index| %>
<li><%= t.rank %> Artist: <%= t.artist %> Track: <%= t.title %></li>
  <% if t.soundcloud_url != "Soundcloud Link Not Available" %>
    <% link = "https://w.soundcloud.com/player/?url=https%3A//" + t.soundcloud_url + "&amp;auto_play=false&amp;hide_related=false&amp;visual=true" %>
    <iframe id="<%= "iframe_ "+ index.to_s %>"  width="80%" height="120" scrolling="no" frameborder="no" src="<%= link %>"></iframe>
  <% else %>
    <li>Soundcloud link not available</li>
  <% end %>
<% end %>
</ul>
